#JSGF V1.0;
grammar visit;
public <visit> = ([volver] [atras] | [me he] equivocado) { this.$value = "-1" } 
                | [somos] <masuno> [personas | visitantes] { this.$value = $masuno }
                | <uno> { this.$value =  "1" };

<uno> = [soy] (un visitante | una persona)
      | solo (yo | uno) ;
