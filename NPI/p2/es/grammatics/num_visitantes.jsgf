#JSGF V1.0;
grammar total_visitantes;
public <total_visitantes> = [somos] <masuno> [personas | visitantes] { this.numVisitors = $masuno }
                          | <uno> { this.numVisitors =  "1" };

<uno> = [soy] (un visitante | una persona)
      | solo (yo | uno) ;

<masuno> = 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 | 10 ;